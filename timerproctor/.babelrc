{
  "presets": [
    "razzle/babel",
    "react-app"
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["styled-components", { "ssr": true }],
    [
      "babel-plugin-root-import",
      {
        "rootPathSuffix": "./src/",
        "rootPathPrefix": "~/"
      }
    ]
  ]
}
